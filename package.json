{
  "name": "bare-runtime",
  "version": "1.11.0",
  "description": "Prebuilt Bare binaries for macOS, iOS, Linux, Android, and Windows",
  "exports": {
    ".": "./index.js",
    "./package": "./package.json",
    "./spawn": "./lib/spawn.js"
  },
  "bin": {
    "bare": "bin/bare"
  },
  "files": [
    "NOTICE",
    "index.js",
    "bin",
    "lib"
  ],
  "workspaces": [
    "npm/*"
  ],
  "imports": {
    "child_process": {
      "bare": "bare-subprocess",
      "default": "child_process"
    },
    "os": {
      "bare": "bare-os",
      "default": "os"
    },
    "path": {
      "bare": "bare-path",
      "default": "path"
    }
  },
  "scripts": {
    "test": "standard"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/holepunchto/bare-runtime.git"
  },
  "author": "Holepunch",
  "license": "Apache-2.0",
  "bugs": {
    "url": "https://github.com/holepunchto/bare-runtime/issues"
  },
  "homepage": "https://github.com/holepunchto/bare-runtime",
  "dependencies": {
    "bare-os": "^3.0.1",
    "bare-path": "^3.0.0",
    "bare-subprocess": "^4.0.2"
  },
  "optionalDependencies": {
    "bare-runtime-android-arm": "1.11.0",
    "bare-runtime-android-arm64": "1.11.0",
    "bare-runtime-android-ia32": "1.11.0",
    "bare-runtime-android-x64": "1.11.0",
    "bare-runtime-darwin-arm64": "1.11.0",
    "bare-runtime-darwin-x64": "1.11.0",
    "bare-runtime-ios-arm64": "1.11.0",
    "bare-runtime-ios-arm64-simulator": "1.11.0",
    "bare-runtime-ios-x64-simulator": "1.11.0",
    "bare-runtime-linux-arm64": "1.11.0",
    "bare-runtime-linux-x64": "1.11.0",
    "bare-runtime-win32-arm64": "1.11.0",
    "bare-runtime-win32-x64": "1.11.0"
  },
  "devDependencies": {
    "standard": "^17.0.0"
  }
}
